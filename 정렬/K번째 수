def solution(array, commands):
  answer = []

  for command in commands :
    # print(command)
    i = command[0]
    j = command[1]
    k = command[2]
    temp = array[i-1:j]
    temp = sorted(temp)
    # print(temp)
    answer.append(temp[k-1])
  return answer

def main() :
  array = [1, 5, 2, 6, 3, 7, 4]
  commands =	[[2, 5, 3], [4, 4, 1], [1, 7, 3]]

  # answer = [5, 6, 3]
  answer = solution(array, commands)

  print(answer)

main()
